{
  "compilerOptions": {
  "target": "es6",
  "sourceMap": true,
  "allowJs": true,
  "strictNullChecks": true,
  "noImplicitAny": true,
  "preserveConstEnums": true,
  "esModuleInterop": true,
  "outDir": "$$ts-jest$$",
  "moduleResolution": "node",
  },
  "include": ["src/**/*.ts", "src/**/*.js"],
  "exclude": ["node_modules", "dist"]
}