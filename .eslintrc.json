{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
   "parserOptions": {
    "parser": "@typescript-eslint/parser"
   },
  "extends": [
      "airbnb-base",
      "plugin:@typescript-eslint/recommended"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "linebreak-style": 0,
    "import/prefer-default-export": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0
  }
}